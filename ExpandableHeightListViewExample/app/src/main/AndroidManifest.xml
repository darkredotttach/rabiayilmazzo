<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="paolorotolo.github.com.expandableheightlistviewexample" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DefaultListView"
            android:label="@string/title_activity_default_list_view" >
        </activity>
        <activity
            android:name=".ExpandableListView"
            android:label="@string/title_activity_expandable_list_view" >
        </activity>
    </application>

</manifest>
